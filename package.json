{
    "name": "clickr",
    "version": "1.1.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "npm run build:ios:dev",
        "build:all:dev": "npm run build:ios:dev && npm run build:android:dev",
        "build:all:prod": "npm run build:ios:prod && npm run build:android:prod",
        "build:ios:dev": "ng build clickerapp --configuration development.ios && npx cap sync ios",
        "build:ios:prod": "ng build clickerapp --configuration production.ios && npx cap sync ios",
        "build:android:dev": "ng build clickerapp --configuration development.android && npx cap sync android",
        "build:android:prod": "ng build clickerapp --configuration production.android && npx cap sync android",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "all-tests": "ng test & ng test @libs/home-view & ng test @libs/counter-form & ng test @libs/detail-view & ng test @libs/automation-editor-view",
        "lint": "ng lint",
        "format": "prettier --write \"projects/**/*.{ts,html,scss,json}\"",
        "format:check": "prettier --check \"projects/**/*.{ts,html,scss,json}\"",
        "lint:fix": "eslint \"./projects/**/*.ts\" --fix"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.1.0",
        "@angular/cdk": "^19.1.3",
        "@angular/common": "^19.1.0",
        "@angular/compiler": "^19.1.0",
        "@angular/core": "^19.1.0",
        "@angular/forms": "^19.1.0",
        "@angular/material": "^19.1.3",
        "@angular/platform-browser": "^19.1.0",
        "@angular/platform-browser-dynamic": "^19.1.0",
        "@angular/router": "^19.1.0",
        "@capacitor-community/admob": "^7.0.2",
        "@capacitor/android": "^7.2.0",
        "@capacitor/app": "^7.0.1",
        "@capacitor/core": "^7.1.0",
        "@capacitor/haptics": "^7.0.1",
        "@capacitor/ios": "^7.1.0",
        "@capacitor/keyboard": "^7.0.1",
        "@capacitor/status-bar": "^7.0.1",
        "@ionic/angular": "^8.6.5",
        "date-fns": "^4.1.0",
        "ng-mocks": "^14.13.2",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "uuid": "^11.0.5",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.1.6",
        "@angular-eslint/builder": "19.3.0",
        "@angular-eslint/eslint-plugin": "19.3.0",
        "@angular-eslint/eslint-plugin-template": "19.3.0",
        "@angular-eslint/schematics": "19.3.0",
        "@angular-eslint/template-parser": "19.3.0",
        "@angular/cli": "^19.1.6",
        "@angular/compiler-cli": "^19.1.0",
        "@capacitor/assets": "^3.0.5",
        "@capacitor/cli": "^7.1.0",
        "@types/jasmine": "~5.1.0",
        "@typescript-eslint/eslint-plugin": "7.11.0",
        "@typescript-eslint/parser": "7.11.0",
        "angular-eslint": "^19.3.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "jasmine-core": "~5.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lint-staged": "^15.2.2",
        "ng-packagr": "^19.1.0",
        "prettier": "^3.2.5",
        "typescript": "~5.7.2"
    }
}
